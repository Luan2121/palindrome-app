  "main": "index.js",
  "scripts": {
    "test": "mocha --require @babel/register 'src/test/**/*.js'",
    "babel-node": "babel-node",
    "dev": "nodemon --exec npm run babel-node -- src/index.js",
    "clean": "rm -rf build && mkdir build",
    "build": "npm run clean && babel -d ./build ./src -s",
